import java.util.*;
public class reverse{
	public static void main(String[] x)
	{
	  Scanner sc=new Scanner(System.in);
	  int a;
	  System.out.println("Enter the number");
	  a=sc.nextInt();
	  int num=0;
	  while(a>0)
	  {
		int b=a%10;
		num=num*10+b;
		a=a/10;
	  }
	  System.out.println(num);
	}
}
