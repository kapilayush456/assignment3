public class array{
	public static void main(String[] x){
		int a[] = new int[8];
		   a[0]=1;
		   a[1]=1;
		   a[2]=1;
		   a[3]=1;
		   a[4]=0;
		   a[5]=0;
		   a[6]=1;
		   a[7]=0;
		System.out.print(a[4]+" ");
		System.out.print(a[5]+" ");
		System.out.print(a[7]+" ");
		System.out.print(a[1]+" ");
		System.out.print(a[2]+" ");
		System.out.print(a[3]+" ");
		System.out.print(a[0]+" ");
		System.out.print(a[6]);
	}
}
